{
  "name": "vue-ecailr",
  "version": "0.0.2",
  "type": "module",
  "files": [
    "lib"
  ],
  "types": "./lib/VueEcailr.vue.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/cjboy76/vue-ecailr.git"
  },
  "main": "./lib/vue-ecailr.js",
  "module": "./lib/vue-ecailr.js",
  "exports": {
    ".": {
      "import": "./dist/vue-ecailr.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "vite build --mode lib && vue-tsc --emitDeclarationOnly && mv dist/components/VueEcailr.vue.d.ts dist/types && rm dist/components",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/node": "^18.8.3",
    "@vitejs/plugin-vue": "^3.1.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vue-tsc": "^0.40.4"
  }
}
